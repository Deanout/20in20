<!-- Header -->
<header class="masthead">
    <div class="container">
        <div class="intro-text">
            <div class="intro-lead-in">My Portfolio</div>
            <div class="intro-heading text-uppercase">I've got a lot of dev experience!</div>
            <a class="btn btn-primary btn-xl text-uppercase js-scroll-trigger" href="#services">Check it out!</a>
        </div>
    </div>
</header>

<!-- Services -->
<section class="page-section" id="services">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2 class="section-heading text-uppercase">Skills</h2>
                <h3 class="section-subheading text-muted">Here's my top 3 skills</h3>
            </div>
        </div>
        <div class="row text-center">
            <% @skills.each do |skill| %>
            <div class="col-md-4">
                <span class="fa-stack fa-4x">
                    <i class="fas fa-circle fa-stack-2x text-primary"></i>
                    <i class="fas <%= skill.icon%> fa-stack-1x fa-inverse"></i>
                </span>
                <h4 class="service-heading"><%= skill.name %></h4>
                <p class="text-muted"><%= skill.description %></p>
            </div>
            <% end %>
        </div>

    </div>
</section>

